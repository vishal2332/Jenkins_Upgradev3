pipeline {
      agent any
      stages {
           stage('Init') {
                 steps {
                     echo 'Hi this is vishal learning jenkins codepipeline'
                     echo 'i am writing this jenkins file manually'
                 }
           }
           stage('Build') {
                 steps {
                       echo "Building the code"
                 }
           }
           stage('Deploy on Staging') {
                 steps {
                       echo "Deploying in staging area"
                 }
           }
      }


}